function simpleArraySum(n, ar) {
    // Complete this function
    sum = 0;
    i = 0;
    while (i < n)
        {
            sum = sum + ar[i];
            i = i + 1;
        }
    return sum;
}

function main() {
    var n = parseInt(readLine());
    ar = readLine().split(' ');
    ar = ar.map(Number);
    var result = simpleArraySum(n, ar);
    process.stdout.write("" + result + "\n");

}
